{
  "eslint.packageManager": "pnpm",
  "eslint.run": "onSave",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "typescript",
    "typescriptreact"
  ],
  "files.autoSave": "afterDelay",
  "editor.tabSize": 2,
  "files.eol": "\n",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "stylelint.packageManager": "pnpm",
  "stylelint.enable": true,
  "css.validate": false,
  "files.exclude": {
    "**/.git": false,
    "**/.husky": true
  },
  "stylelint.validate": [
    "css",
    "less",
    "postcss",
    "vue"
  ],
  "unocss.disable": true,
  "unocss.colorPreview": true,
}